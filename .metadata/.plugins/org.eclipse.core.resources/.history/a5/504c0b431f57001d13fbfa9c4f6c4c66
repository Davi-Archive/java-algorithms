import java.util.Scanner;

public class Loops {

	public static void main(String[] args) {
		Scanner teclado = new Scanner(System.in);
		System.out.println("Insira um numero: ");
		int n1 = teclado.nextInt();
		int i = 0;
		while(i<n1) {
		System.out.println("O nÃºmero inserido foi: "+n1);
		}
		teclado.close();

	}

}
